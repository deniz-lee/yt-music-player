# Uncomment the next line to define a global platform for your project

target 'YTMusicPlayer (iOS)' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  use_modular_headers!
  platform :ios, '12.0'

  # Pods for YTMusicPlayer (iOS)
  pod 'AFNetworking'
  pod 'GoogleSignIn'

end

target 'YTMusicPlayer (macOS)' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  use_modular_headers!
  platform :osx, '10.15'

  # Pods for YTMusicPlayer (macOS)
  pod 'AFNetworking'
  pod 'GoogleSignIn'

end
